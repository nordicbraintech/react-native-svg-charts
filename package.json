{
    "name": "react-native-svg-charts",
    "version": "5.4.0",
    "private": false,
    "description": "Customizable charts (Line, Bar, Area, Pie, Circle, Progress) for React Native",
    "main": "lib/module/index.js",
    "module": "lib/module/index.js",
    "react-native": "src/index.js",
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint .",
        "storybook": "storybook start -p 7008",
        "prep:bob": "bob build",
				"prep": "husky install .husky"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint"
    },
    "keywords": [
        "react-native",
        "react-component",
        "react-native-component",
        "react",
        "mobile",
        "ios",
        "android",
        "vector",
        "charts",
        "chart",
        "d3"
    ],
    "author": {
        "name": "Jesper Lekland",
        "email": "jesper.lekland@push2prod.se"
    },
    "homepage": "https://github.com/JesperLekland/react-native-svg-charts",
    "bugs": {
        "url": "https://github.com/JesperLekland/react-native-svg-charts/issues"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/JesperLekland/react-native-svg-charts.git"
    },
    "license": "MIT",
    "dependencies": {
        "d3-array": "^3.2.0",
        "d3-interpolate-path": "2.x.x",
        "d3-scale": "^3.0.6",
        "d3-shape": "^3.0.6",
        "prop-types": "^15.6.0"
    },
    "peerDependencies": {
        "react": "18.0.0",
        "react-native": ">=0.69.10",
        "react-native-svg": "^12.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/eslint-parser": "^7.21.8",
        "@storybook/addon-actions": "5.x.x",
        "@storybook/addon-knobs": "^5.x.x",
        "@storybook/addon-links": "5.x.x",
        "@storybook/react-native": "5.x.x",
        "babel-jest": "28.1.3",
        "date-fns": "2.x.x",
        "eslint": "^8.22.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-native": "^2.3.2",
        "husky": "^8.0.3",
        "jest": "27.x.x",
        "lint-staged": "^7.1.3",
        "metro-react-native-babel-preset": "^0.70.3",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-native": "0.69.10",
        "react-native-builder-bob": "^0.20.4",
        "react-native-svg": "^12.1.1",
        "react-test-renderer": "18.0.0"
    },
    "jest": {
        "preset": "react-native",
        "modulePathIgnorePatterns": [
            "<rootDir>/lib/"
        ]
    },
    "files": [
        "src",
        "lib"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "module"
        ]
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ]
}
